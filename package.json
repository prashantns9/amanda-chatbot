{
  "name": "amanda-chatbot",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start-web": "http-server -p 8080 -c-1 dist/amanda-chatbot",
    "start-desktop": "electron .",
    "build-web": "ng build --prod",
    "build-win": "electron-packager ./ amanda-chatbot --platform=win32 --arch=x64",
    "build-mac": "electron-packager ./ amanda-chatbot --platform=darwin --arch=x64",
    "build-linux": "electron-packager ./ amanda-chatbot --platform=linux --arch=x64",
    "postinstall": "ng build --aot --prod"
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/pwa": "^0.801.1",
    "@angular/router": "~8.1.1",
    "@angular/service-worker": "~8.1.1",
    "bootstrap": "^4.3.1",
    "electron": "^5.0.6",
    "express": "^4.17.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "api-ai-javascript": "^2.0.0-beta.21",
    "codelyzer": "^5.0.0",
    "electron-packager": "^14.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
